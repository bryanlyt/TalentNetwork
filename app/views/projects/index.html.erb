<div class="page-header">
  <h1>
    Available Projects
  </h1>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th>
        <%= t(:name) %>
      </th>
      <th>
        <%= t(:location) %>
      </th>
      <th>
        <%= t(:creator) %>
      </th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td>
          <%= link_to project, project %>
        </td>
        <td>
          <%= project.location %>
        </td>
        <td>
          <%= project.creator %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
